doctype html
html
  head
    meta(charset="utf-8")
    meta(content="IE=edge,chrome=1", http-equiv="X-UA-Compatible")
    meta(name="viewport", content="width=device-width, initial-scale=1, maximum-scale=1")
    title API Reference
    link(href="stylesheets/screen.css", rel="stylesheet", type="text/css", media="screen")
    link(href="stylesheets/print.css", rel="stylesheet", type="text/css", media="print")
    script(src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js")
    script(src="javascripts/all.js", type="text/javascript")
    script
      !$(function(){setupLanguages(["shell","javascript","ruby","python"]);}
  body.index
    a#nav-button(href="#")
        span
          | NAV
          img(src="images/navbar.png")
      .tocify-wrapper
        img(src="images/logo.png")
        .lang-selector
          a(href="#", data-language-name="shell") bash
          a(href="#", data-language-name="javascript") javascript
          a(href="#", data-language-name="csharp") C#
          a(href="#", data-language-name="ruby") ruby
          a(href="#", data-language-name="python") python
        .search
          input#input-search.search(type="text", placeholder="Search")
        ul.search-results
        #toc
        ul.toc-footer
          li
            a(href="#") Sign Up for a Developer Key
          li
            a(href="http://github.com/tripit/slate") Documentation Powered by Slate
    .page-wrapper
      block content
      .dark-box
        .lang-selector
          a(href="#", data-language-name="bash") Bash
          a(href="#", data-language-name="javascript") JavaScript